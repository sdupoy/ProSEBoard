		<mapper resource="/fr/eseo/ld/proseboard/mybatis/mappers/ConsultingMapper.xml"/>
		<mapper resource="/fr/eseo/ld/proseboard/mybatis/mappers/NotificationMapper.xml"/>
		<mapper resource="/fr/eseo/ld/proseboard/mybatis/mappers/StatusUserMapper.xml"/>		
		<mapper resource="/fr/eseo/ld/proseboard/mybatis/mappers/BillMapper.xml"/>
		<mapper resource="/fr/eseo/ld/proseboard/mybatis/mappers/AccountHistoryMapper.xml"/>
		<mapper resource="/fr/eseo/ld/proseboard/mybatis/mappers/TrainingUserMapper.xml"/>
	</mappers>
