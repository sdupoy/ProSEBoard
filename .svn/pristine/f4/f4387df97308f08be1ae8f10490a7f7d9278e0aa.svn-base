package fr.eseo.ld.proseboard.models;

import static org.junit.Assert.*;

import org.junit.Test;

public class PayTeamTest {

	@Test
	public void testEmptyConstructor() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
	}

	@Test
	public void testGetIdTeam() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
		pt.setIdTeam(12L);
		assertEquals(12L,pt.getIdTeam(), 0.1);
	}
	
	@Test
	public void testGetIdTeamLeader() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
		pt.setIdTeamLeader(12L);
		assertEquals(12L,pt.getIdTeamLeader(), 0.1);
	}
	
	@Test
	public void testGetIdClient() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
		pt.setIdClient(12L);
		assertEquals(12L,pt.getIdClient(), 0.1);
	}
	
	@Test
	public void testGetIdTeamAccount() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
		pt.setIdTeamAccount(12L);
		assertEquals(12L,pt.getIdTeamAccount(), 0.1);
	}
	
	@Test
	public void testGetIdClientAccount() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
		pt.setIdClientAccount(12L);
		assertEquals(12L,pt.getIdClientAccount(), 0.1);
	}
	
	@Test
	public void testGetAmount() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
		pt.setAmount(1200);
		assertEquals(1200,pt.getAmount());
	}
	
	@Test
	public void testGetTeamName() {
		PayTeam pt = new PayTeam();
		assertNotNull(pt);
		pt.setTeamName("Equipe");
		assertEquals("Equipe",pt.getTeamName());
	}
}
